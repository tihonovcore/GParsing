@code
def x = Array[Int](4);
x[0] = 1;
x[1] = 9;
x[2] = 8;
x[3] = 4;

def y = Array[Int](2);
y[0] = 5;
y[1] = 7;

def xy = concat(x, y);
print xy[0];
print xy[1];
print xy[2];
print xy[3];
print xy[4];
print xy[5];

println "";

def xy2 : Array[Int];
xy2 = concat(x, y);
print xy2[0];
print xy2[1];
print xy2[2];
print xy2[3];
print xy2[4];
print xy2[5];

